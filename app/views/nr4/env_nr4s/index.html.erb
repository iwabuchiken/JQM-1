<%#= button_to "Backup DB", :action => 'backup_db', {:method => 'get'}%><!-- form action="/nr4/env_nr4s/backup_db?method=get" class="button_to" method="post" -->


<% unless @env_nr4s.size > 0 %>
	<%= link_to 'New Env nr4', new_env_nr4_path %>
<% end %>

